console.log("connected");

var questions = [
  {
    prompt: "What color are oranges?\n(a) Orange\n (b) Purple",
    answer: "a",
  },
];

var score = 0;
for (var i = 0; i < questions.length; i++) {
  var response = window.prompt(questions[i].prompt);
  if (response == questions[i].answer) {
    score++;
    alert("Correct");
  } else {
    alert("Wrong");
  }
}
